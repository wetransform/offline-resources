	<xs:complexType name="PollAssociationType">
		<xs:annotation>
			<xs:documentation>Used to link between the various components of the polling district contained within the EML 150 content instance.  Id attribute points to the Id of the associated information, and idType provides actual target. Implementers are responsible for documenting how they are using these mechanisms in their specific data content.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:token">
				<xs:attribute name="Id" type="xs:token">
					<xs:annotation>
						<xs:documentation>Value of the Id of the content being associated to within this EML 150 content structure..</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="IdType" use="optional">
					<xs:annotation>
						<xs:documentation>Types of the associated information from the overall EML 150 content structure referenced by the Id attribute value.</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:token">
							<xs:enumeration value="district"/>
							<xs:enumeration value="pollingPlace"/>
							<xs:enumeration value="pollingDivision"/>
							<xs:enumeration value="pollingStation"/>
							<xs:enumeration value="placeResources"/>
							<xs:enumeration value="geoBoundary"/>
							<xs:enumeration value="geoLocation"/>
							<xs:enumeration value="geoDistrict"/>
							<xs:enumeration value="boundary"/>
							<xs:enumeration value="ballot"/>
							<xs:enumeration value="election"/>
							<xs:enumeration value="contest"/>
							<xs:enumeration value="other"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
				<xs:attribute name="note" use="optional" type="xs:string">
					<xs:annotation><xs:documentation>Optional implementers note documenting relationships being used in the data content.</xs:documentation></xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
