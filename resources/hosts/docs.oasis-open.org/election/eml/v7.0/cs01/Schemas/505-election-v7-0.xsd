<!--
  EML 505 Schema - Election Details

  History: 7.0 Edited by David RR Webber, May 2011
                - major release revision

  -->
<xs:schema targetNamespace="urn:oasis:names:tc:evs:schema:eml" elementFormDefault="qualified"
  attributeFormDefault="unqualified" version="7.0" id="eml-505"
  xmlns="urn:oasis:names:tc:evs:schema:eml" xmlns:xal="urn:oasis:names:tc:ciq:xal:4"
  xmlns:xnl="urn:oasis:names:tc:ciq:xnl:4" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:annotation>
    <xs:documentation> 
			Specification Name: OASIS EML TC - Election Markup Language (EML) 
			Description: This schema here is a member of a library of the XML schemas used in EML. The XML schemas define the formal structures of the election data that can be processed.
			Produced by: OASIS Election Technical Committee
			URL: http://www.oasis-open.org/committees/election
			Version: 7.0  
			Status: Committee Draft 
			Copyright: 2001-11, OASIS, http://www.oasis-open.org
			Conformance: This is a base implementation schema - actual localization schema for use may be derived from this schema - but may not contradict the structure definitions provided here.
		</xs:documentation>
  </xs:annotation>
  <!-- Import the common definitions, eml-core and external schema -->
  <xs:include schemaLocation="110-505-include-v7-0.xsd"/>
  <!-- Import these external here again - because schema will not resolve references here without them -->
  <xs:import namespace="urn:oasis:names:tc:ciq:xal:4" schemaLocation="external/CIQ-xAL-v4.xsd"/>
  <xs:import namespace="urn:oasis:names:tc:ciq:xnl:4" schemaLocation="external/CIQ-xNL-v4.xsd"/>
  <xs:element name="EML">
    <xs:annotation>
      <xs:documentation>EML 505 structure definition.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="EMLstructure">
          <xs:sequence>
            <xs:element ref="ElectionDetails">
              <xs:annotation>
                <xs:documentation>Collection of information regarding an election.</xs:documentation>
              </xs:annotation>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="Id" fixed="505" use="required"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
    <xs:unique name="DisplayOrder">
      <xs:selector xpath=".//Contest/ContestIdentifier/*"/>
      <xs:field xpath="DisplayOrder"/>
    </xs:unique>
  </xs:element>
  <xs:element name="ElectionDetails">
    <xs:annotation>
      <xs:documentation>Collection of information regarding an election.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="ElectionControl" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="ElectionAdministration" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="ElectionParticipation" minOccurs="0" maxOccurs="1"/>
        <xs:element name="Election" maxOccurs="unbounded">
          <xs:annotation>
            <xs:documentation>Details regarding this election event.</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Localities" minOccurs="1" maxOccurs="1" id="d2e2979">
                <xs:annotation>
                  <xs:documentation>Overall geographical locations for which the election is to be held.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="LocalityEntity" minOccurs="1" maxOccurs="unbounded"
                      id="d2e2992">
                      <xs:annotation>
                        <xs:documentation>Details of this specific locality within the election.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:annotation>
                            <xs:documentation>Details of Locality which is a named densely populated area  (a place) such as town, village, suburb, etc. A locality composes of many individual addresses.  Many localities exist in an administrative area or a sub adminisrative area. A locality can also have sub localities. For example, a municipality locality can have many villages associated with it which are sub localities. Example: Tamil Nadu State, Erode District, Bhavani Taluk, Paruvachi Village is a valid address in India. Tamil Nadu is the Administrative Area, Erode is the sub admin area, Bhavani is the locality, and Paruvachi is the sub locality 
                                    Target: .//Localities/Locality | LocalityID - Name: LocalityKeyRef</xs:documentation>
                          </xs:annotation>
                          <xs:element name="LocalityName" minOccurs="1" maxOccurs="1"
                            type="LocalityNameDefinition" id="d2e3020">
                            <xs:annotation>
                              <xs:documentation>LocalityNameDefinition</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="LocalityType" minOccurs="1" maxOccurs="1"
                            type="LocalityTypeDefinition" id="d2e3041">
                            <xs:annotation>
                              <xs:documentation>LocalityTypeDefinition</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="LocalityID" minOccurs="1" maxOccurs="1"
                            type="LocalityIDDefinition" id="d2e3055">
                            <xs:annotation>
                              <xs:documentation>LocalityIDDefinition</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="BallotID" minOccurs="0" maxOccurs="unbounded"
                            type="BallotIDDefinition" id="d2e3070">
                            <xs:annotation>
                              <xs:documentation>Identifier reference to the ballot(s) associated with this jurisdiction that the voter can cast.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                        <xs:attribute name="id" type="xs:token" id="d2e3007" use="required">
                          <xs:annotation>
                            <xs:documentation>Globally unique identifier</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="Precincts" minOccurs="0" maxOccurs="1" id="d2e3086">
                      <xs:annotation>
                        <xs:documentation>Electorial grouping of voters within localities.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Precinct" minOccurs="1" maxOccurs="unbounded"
                            id="d2e3098">
                            <xs:annotation>
                              <xs:documentation>Specific group of voters within a denoted voting boundary area. 
Target: .//Precincts/Precinct | @id - Name: PrecinctKeyRef</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="PrecinctName" minOccurs="1" maxOccurs="1"
                                  type="PrecinctNameDefinition" id="d2e3126">
                                  <xs:annotation>
                                    <xs:documentation>PrecinctNameDefinition</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="LocalityID" minOccurs="1" maxOccurs="1"
                                  type="LocalityIDDefinition" id="d2e3140">
                                  <xs:annotation>
                                    <xs:documentation>LocalityIDDefinition</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="ElectoralDistrictID" minOccurs="0" maxOccurs="1"
                                  type="ElectoralDistrictIDDefinition" id="d2e3154">
                                  <xs:annotation>
                                    <xs:documentation>ElectoralDistrictIDDefinition</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="PollingLocationID" minOccurs="0" maxOccurs="1"
                                  type="PollingLocationIDDefinition" id="d2e3168">
                                  <xs:annotation>
                                    <xs:documentation>PollingLocationIDDefinition</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="BallotID" minOccurs="0" maxOccurs="unbounded"
                                  type="BallotIDDefinition" id="d2e3181">
                                  <xs:annotation>
                                    <xs:documentation>Identifier reference to the ballot(s) associated with this jurisdiction that the voter can cast.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                              <xs:attribute name="id" type="xs:token" id="d2e3113" use="required">
                                <xs:annotation>
                                  <xs:documentation>Globally unique identifier</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="PrecinctSplits" minOccurs="0" maxOccurs="1" id="d2e3198">
                      <xs:annotation>
                        <xs:documentation>Due to zoning requirements electoral precincts may be split between different districts.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="PrecinctSplit" minOccurs="1" maxOccurs="unbounded"
                            id="d2e3210">
                            <xs:annotation>
                              <xs:documentation>Details of a split precinct and the districts it pertains to.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="SplitPrecinctName" minOccurs="1" maxOccurs="1"
                                  type="SplitPrecinctNameDefinition" id="d2e3235">
                                  <xs:annotation>
                                    <xs:documentation>SplitPrecinctNameDefinition</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="LocalityID" minOccurs="0" maxOccurs="1"
                                  type="LocalityIDDefinition" id="d2e3249"/>
                                <xs:element name="PrecinctID" minOccurs="1" maxOccurs="1"
                                  type="PrecinctIDDefinition" id="d2e3256">
                                  <xs:annotation>
                                    <xs:documentation>PrecinctIDDefinition</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="ElectoralDistrictID" minOccurs="0"
                                  maxOccurs="unbounded" type="ElectoralDistrictIDDefinition"
                                  id="d2e3271">
                                  <xs:annotation>
                                    <xs:documentation>ElectoralDistrictIDDefinition</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="PollingLocationID" minOccurs="0" maxOccurs="1"
                                  type="PollingLocationIDDefinition" id="d2e3284"/>
                                <xs:element name="BallotID" minOccurs="0" maxOccurs="unbounded"
                                  type="BallotIDDefinition" id="d2e3291">
                                  <xs:annotation>
                                    <xs:documentation>Identifier reference to the ballot(s) associated with this jurisdiction that the voter can cast.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                              <xs:attribute name="id" type="xs:token" id="d2e3222" use="required">
                                <xs:annotation>
                                  <xs:documentation>Globally unique identifier</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="Districts" minOccurs="1" maxOccurs="1" id="d2e3309">
                <xs:annotation>
                  <xs:documentation>Political areas and grouping of geographical locations into distinct representative areas that will be appointed delegates.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ElectoralDistrict" minOccurs="1" maxOccurs="unbounded"
                      id="d2e3322">
                      <xs:annotation>
                        <xs:documentation>Specific electoral district for which a representative will be appointed. 
Target: .//ElectoralDistrict/District | DistrictName - Name: DistrictKeyRef</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="DistrictName" minOccurs="1" maxOccurs="1"
                            type="DistrictNameDefinition" id="d2e3350">
                            <xs:annotation>
                              <xs:documentation>DistrictNameDefinition</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="DistrictType" minOccurs="1" maxOccurs="1"
                            type="DistrictTypeDefinition" id="d2e3364">
                            <xs:annotation>
                              <xs:documentation>DistrictTypeDefinition</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                        <xs:attribute name="id" type="xs:token" id="d2e3337" use="required">
                          <xs:annotation>
                            <xs:documentation>Globally unique identifier</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="Contests" minOccurs="1" maxOccurs="1" id="d2e3380">
                <xs:annotation>
                  <xs:documentation>Details of the balloted items for the election.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Contest" minOccurs="1" maxOccurs="unbounded" id="d2e3393">
                      <xs:annotation>
                        <xs:documentation>Specific contest for election designated by district and ballot.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="ContestIdentifier" type="ContestIdentifierStructure"
                            minOccurs="1" maxOccurs="1" id="d2e3454"/>
                          <xs:element name="ContestScope" minOccurs="0" maxOccurs="1"
                            type="ContestScopeDefinition" id="d2e3518">
                            <xs:annotation>
                              <xs:documentation>Applicability of the contest and jurisdiction.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="ElectionType" minOccurs="0" maxOccurs="1"
                            type="ElectionTypeDefinition" id="d2e3528">
                            <xs:annotation>
                              <xs:documentation>The political significance of the election; e.g. General Election, Mid-Term, Special, Local and so on.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="ReportingUnitIdentifier"
                            type="ReportingUnitIdentifierStructure" minOccurs="0"
                            maxOccurs="unbounded" id="d2e3543"/>
                          <xs:element name="Qualified" type="QualifiedDefinition" minOccurs="0"
                            maxOccurs="1" id="d2e3592">
                            <xs:annotation>
                              <xs:documentation>Denotes if the item has been qualified.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="HowToVote" type="MessagesStructure" minOccurs="0"
                            maxOccurs="1" id="d2e3607">
                            <xs:annotation>
                              <xs:documentation>How a vote is cast.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="Rotation" type="YesNoType" minOccurs="0" maxOccurs="1"
                            id="d2e3681">
                            <xs:annotation>
                              <xs:documentation>If rotation is applicable.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="RotationMethod" minOccurs="0" maxOccurs="1"
                            type="RotationMethodDefinition" id="d2e3698">
                            <xs:annotation>
                              <xs:documentation>Specific rotation method applicable.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="VotingMethod" type="VotingMethodType" minOccurs="0"
                            maxOccurs="1" id="d2e3708">
                            <xs:annotation>
                              <xs:documentation>Formal description of the voting method.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="ChoiceType" minOccurs="0" maxOccurs="1"
                            type="ChoiceTypeDefinition" id="d2e3728">
                            <xs:annotation>
                              <xs:documentation>Type of choice for the applicable items.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="MaxVotes" minOccurs="0" maxOccurs="1" id="d2e3737">
                            <xs:annotation>
                              <xs:documentation>Maximum number of vote selections.</xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:integer">
                                <xs:totalDigits value="6"/>
                                <xs:fractionDigits value="0"/>
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element name="MinVotes" minOccurs="0" maxOccurs="1" id="d2e3750">
                            <xs:annotation>
                              <xs:documentation>Minimum number of vote selections.</xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:integer">
                                <xs:totalDigits value="6"/>
                                <xs:fractionDigits value="0"/>
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element name="MaxWriteIn" minOccurs="0" maxOccurs="1" id="d2e3763">
                            <xs:annotation>
                              <xs:documentation>The number of write in lines applicable.</xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:integer">
                                <xs:totalDigits value="6"/>
                                <xs:fractionDigits value="0"/>
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element name="Blocked" type="BlockedDefinition" minOccurs="0"
                            maxOccurs="1" id="d2e3774">
                            <xs:annotation>
                              <xs:documentation>If this ballot item is blocked for this voter by entitlement rule.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="BallotChoices" minOccurs="1" maxOccurs="1" id="d2e3806">
                            <xs:annotation>
                              <xs:documentation>Specific set of ballot choices.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="Candidate" type="CandidateStructure" minOccurs="0"
                                  maxOccurs="unbounded" id="d2e3831">
                                  <xs:annotation>
                                    <xs:documentation>Candidate details.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                                <xs:element name="Affiliation" minOccurs="0" maxOccurs="unbounded"
                                  id="d2e6551">
                                  <xs:annotation>
                                    <xs:documentation>Affliation(s) associated with this candidate.</xs:documentation>
                                  </xs:annotation>
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="AffiliationIdentifier"
                                        type="AffiliationIdentifierStructure" minOccurs="1"
                                        maxOccurs="1" id="d2e6558"/>
                                      <xs:element name="Type" minOccurs="1" maxOccurs="1"
                                        type="TypeDefinition" id="d2e6623">
                                        <xs:annotation>
                                          <xs:documentation>TypeDefinition</xs:documentation>
                                        </xs:annotation>
                                      </xs:element>
                                      <xs:element name="Description" minOccurs="0" maxOccurs="1"
                                        type="DescriptionDefinition" id="d2e6636">
                                        <xs:annotation>
                                          <xs:documentation>DescriptionDefinition</xs:documentation>
                                        </xs:annotation>
                                      </xs:element>
                                      <xs:element name="Logo" type="BinaryItemStructure"
                                        minOccurs="0" maxOccurs="unbounded" id="d2e6651"/>
                                      <xs:element name="CandidateIdentifier"
                                        type="CandidateIdentifierStructure" minOccurs="0"
                                        maxOccurs="unbounded" id="d2e6769"/>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element name="ReferendumOptionIdentifier"
                                  type="ReferendumOptionIdentifierStructure" minOccurs="0"
                                  maxOccurs="unbounded" id="d2e6913">
                                  <xs:annotation>
                                    <xs:documentation>Referendum, proposition or issue item reference.</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                              <xs:attribute name="Contested" type="xs:token" id="d2e3813"
                                use="optional">
                                <xs:annotation>
                                  <xs:documentation>Denotes if this is a contested ballot choice, e.g. there are more candidates than positions available and hence voting is applicable.</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="Partisan" type="xs:token" id="d2e3821"
                                use="optional">
                                <xs:annotation>
                                  <xs:documentation>Denotes the type of partisan voting permitted if applicable.</xs:documentation>
                                </xs:annotation>
                              </xs:attribute>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="Messages" type="MessagesStructure" minOccurs="0"
                            maxOccurs="unbounded" id="d2e6969">
                            <xs:annotation>
                              <xs:documentation>Optional messages about these items that the voter may need.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="ElectoralDistrictID" minOccurs="0" maxOccurs="1"
                            type="ElectoralDistrictIDDefinition" id="d2e7046">
                            <xs:annotation>
                              <xs:documentation>ElectoralDistrictIDDefinition</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="PrimaryParty" minOccurs="0" maxOccurs="1"
                            type="PrimaryPartyDefinition" id="d2e7059">
                            <xs:annotation>
                              <xs:documentation>PrimaryPartyDefinition</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="ContestOffice" minOccurs="0" maxOccurs="1"
                            type="ContestOfficeDefinition" id="d2e7073">
                            <xs:annotation>
                              <xs:documentation>ContestOfficeDefinition</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="FilingCloseDate" minOccurs="0" maxOccurs="1"
                            type="FilingCloseDateDefinition" id="d2e7086">
                            <xs:annotation>
                              <xs:documentation>Date on which the filing of items will be formally closed.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="BallotID" minOccurs="1" maxOccurs="unbounded"
                            type="BallotIDDefinition" id="d2e7095">
                            <xs:annotation>
                              <xs:documentation>BallotIDDefinition associated with this contest.</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element name="BallotPlacement" minOccurs="0" maxOccurs="1"
                            type="BallotPlacementDefinition" id="d2e7109">
                            <xs:annotation>
                              <xs:documentation>BallotPlacementDefinition</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="DisplayOrder" type="xs:integer" id="d2e3428" use="optional">
                    <xs:annotation>
                      <xs:documentation>Optional number to control the order the item is displayed in when presented visually such as in a ballot form, screen or report (aka ballot placement).</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="Completed" type="xs:token" id="d2e3446" use="optional">
                    <xs:annotation>
                      <xs:documentation>Indicates if the information about the contest is complete.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element name="Ballots" minOccurs="1" maxOccurs="1" id="d2e7124">
                <xs:annotation>
                  <xs:documentation>All the legally approved ballots available for the election.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Ballot" minOccurs="1" maxOccurs="unbounded" id="d2e7137">
                      <xs:annotation>
                        <xs:documentation>BallotStructure - Specific ballot for this election and the associated items that go on to that ballot for voters to cast votes on.</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="CandidateEntry" minOccurs="0" maxOccurs="unbounded"
                            id="d2e7165">
                            <xs:annotation>
                              <xs:documentation>An entry describing a candidate.</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="CandidateIdentifier" minOccurs="1" maxOccurs="1"
                                  id="d2e7173">
                                  <xs:annotation>
                                    <xs:documentation>Specific identifier unique to this candidate.</xs:documentation>
                                  </xs:annotation>
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="CandidateName" type="PersonNameType"
                                        minOccurs="0" maxOccurs="1" id="d2e7248">
                                        <xs:annotation>
                                          <xs:documentation>Name as it appears on the ballot for the candidate.</xs:documentation>
                                        </xs:annotation>
                                      </xs:element>
                                      <xs:element name="KnownAs" minOccurs="0" maxOccurs="1"
                                        type="KnownAsDefinition" id="d2e7272">
                                        <xs:annotation>
                                          <xs:documentation>Alternate popular name for the candidate.</xs:documentation>
                                        </xs:annotation>
                                      </xs:element>
                                      <xs:element name="CandidateGender" type="GenderType"
                                        minOccurs="0" maxOccurs="1" id="d2e7285"/>
                                      <xs:element name="ShortCode" minOccurs="0" maxOccurs="1"
                                        type="ShortCodeType" id="d2e7309"/>
                                      <xs:element name="IsWriteIn" minOccurs="0" maxOccurs="1"
                                        type="IsWriteInDefinition" id="d2e7329">
                                        <xs:annotation>
