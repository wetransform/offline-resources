          <gml:identifier codeSpace="urn:xplan">urn:xplan:enum:xplan:XP_VerbundenerPlan:aenderungsArt:XP_Aenderungsarten:1000</gml:identifier>
          <gml:name>Aenderung</gml:name>
          <gml:name codeSpace="lesbarerName">Änderung</gml:name>
        </ListedValueDefinition>
      </gml:dictionaryEntry>
      <gml:dictionaryEntry>
        <ListedValueDefinition gml:id="xplan_XP_VerbundenerPlan_aenderungsArt_XP_Aenderungsarten_10000">
          <gml:description>Einzelne Planinhalte im Planungsbereich werden durch neue Inhalte ersetzt.</gml:description>
