    </annotation>
    <complexContent>
      <extension base="xplan:BP_UeberlagerungsobjektType">
        <sequence>
          <element name="zweckbestimmung" minOccurs="0" maxOccurs="unbounded" type="xplan:BP_KomplexeZweckbestNebenanlagenPropertyType">
