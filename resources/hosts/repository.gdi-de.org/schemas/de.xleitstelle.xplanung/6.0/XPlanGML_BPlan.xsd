        </annotation>
      </enumeration>
      <enumeration value="2300">
        <!--BehoerdenBeteiligung-->
        <annotation>
          <documentation>Die Beteiligung der Behörden wird durchgeführt  (§4 Abs. 2 BauGB).</documentation>
        </annotation>
      </enumeration>
      <enumeration value="2400">
        <!--OeffentlicheAuslegung-->
