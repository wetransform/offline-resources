        <!--Arkade-->
        <annotation>
          <documentation>Arkade</documentation>
        </annotation>
      </enumeration>
      <enumeration value="1500">
        <!--Luftgeschoss-->
        <annotation>
          <documentation>Luftgeschoss</documentation>
        </annotation>
      </enumeration>
