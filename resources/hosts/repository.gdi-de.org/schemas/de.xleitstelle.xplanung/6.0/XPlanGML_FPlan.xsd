            <annotation>
              <documentation>Art der zulässigen Abweichung von der BauNVO.</documentation>
            </annotation>
          </element>
          <element ref="xplan:_GenericApplicationPropertyOfFP_BebauungsFlaeche" minOccurs="0" maxOccurs="unbounded" />
