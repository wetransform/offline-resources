      <value id="http://inspire.ec.europa.eu/codelist/FossilFuelValue/naturalGas">
         <language>en</language>
         <created>2015-08-18 17:29 PM CEST</created>
         <thisversion>http://inspire.ec.europa.eu/codelist/FossilFuelValue/naturalGas:1</thisversion>
         <latestversion>http://inspire.ec.europa.eu/codelist/FossilFuelValue/naturalGas</latestversion>
         <label xml:lang="en">natural gas</label>
         <definition xml:lang="en">Gases occurring in underground deposits, whether liquefied or gaseous, consisting mainly of methane.</definition>
         <description xml:lang="en">Peat used for non-energy purposes is not included.</description>
         <codelist id="http://inspire.ec.europa.eu/codelist/FossilFuelValue">
