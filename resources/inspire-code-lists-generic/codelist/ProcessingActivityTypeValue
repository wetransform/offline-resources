      <value id="http://inspire.ec.europa.eu/codelist/ProcessingActivityTypeValue/magneticElecromagneticSeparation">
         <language>en</language>
         <created>2015-08-18 18:13 PM CEST</created>
         <thisversion>http://inspire.ec.europa.eu/codelist/ProcessingActivityTypeValue/magneticElecromagneticSeparation:1</thisversion>
