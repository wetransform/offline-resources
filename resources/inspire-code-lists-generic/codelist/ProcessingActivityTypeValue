            <label xml:lang="en">Mineral Resources</label>
         </applicationschema>
         <theme id="http://inspire.ec.europa.eu/theme/mr">
            <label xml:lang="en">Mineral resources</label>
         </theme>
         <governance-level id="http://inspire.ec.europa.eu/registry/governance-level/eu-technical">
            <label xml:lang="en">Technical (EU)</label>
         </governance-level>
         <status id="http://inspire.ec.europa.eu/registry/status/valid">
            <label xml:lang="en">Valid</label>
         </status>
