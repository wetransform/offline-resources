      <value id="http://inspire.ec.europa.eu/codelist/ProcessingActivityTypeValue/centrifugalGravitySeparation">
         <language>en</language>
         <created>2015-08-18 18:13 PM CEST</created>
         <thisversion>http://inspire.ec.europa.eu/codelist/ProcessingActivityTypeValue/centrifugalGravitySeparation:1</thisversion>
         <latestversion>http://inspire.ec.europa.eu/codelist/ProcessingActivityTypeValue/centrifugalGravitySeparation</latestversion>
         <label xml:lang="en">centrifugal gravity separation</label>
         <definition xml:lang="en">Classifying or thickening machine in which centrifugal force is of the essence: it causes the particles in the pulp movement against the sides of a bowl or basket.</definition>
         <codelist id="http://inspire.ec.europa.eu/codelist/ProcessingActivityTypeValue">
            <label xml:lang="en">Processing Activity Type</label>
         </codelist>
         <parents>
            <parent id="http://inspire.ec.europa.eu/codelist/ProcessingActivityTypeValue/physicalTreatment">
               <label xml:lang="en">physical treatment</label>
