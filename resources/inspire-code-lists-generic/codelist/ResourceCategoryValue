      <value id="http://inspire.ec.europa.eu/codelist/ResourceCategoryValue/inferredMineralResource">
         <language>en</language>
         <created>2015-08-18 17:35 PM UTC</created>
         <thisversion>http://inspire.ec.europa.eu/codelist/ResourceCategoryValue/inferredMineralResource:2</thisversion>
