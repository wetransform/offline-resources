      <value id="http://inspire.ec.europa.eu/codelist/WaterAppurtenanceTypeValue/meter">
         <language>en</language>
         <created>2015-08-18 17:38 PM UTC</created>
         <thisversion>http://inspire.ec.europa.eu/codelist/WaterAppurtenanceTypeValue/meter:1</thisversion>
