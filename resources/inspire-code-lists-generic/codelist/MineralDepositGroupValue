      <value id="http://inspire.ec.europa.eu/codelist/MineralDepositGroupValue/chemicalSediment">
         <language>en</language>
         <created>2015-08-18 17:34 PM CEST</created>
         <thisversion>http://inspire.ec.europa.eu/codelist/MineralDepositGroupValue/chemicalSediment:1</thisversion>
