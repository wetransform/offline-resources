      <value id="http://inspire.ec.europa.eu/codelist/BreakLineTypeValue/flatAreaBoundary">
         <language>en</language>
         <created>2015-08-18 17:29 PM CEST</created>
         <thisversion>http://inspire.ec.europa.eu/codelist/BreakLineTypeValue/flatAreaBoundary:1</thisversion>
